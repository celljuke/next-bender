{
  "name": "nextjs-fullstack-template",
  "version": "1.0.0",
  "description": "A production-ready Next.js template with authentication, database, and API layer. Use with: npx create-next-app -e celljuke/next-bender",
  "author": "Your Name",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "nextjs-template",
    "create-next-app",
    "react",
    "typescript",
    "prisma",
    "trpc",
    "postgresql",
    "authentication",
    "tailwindcss",
    "shadcn-ui",
    "template",
    "starter",
    "boilerplate"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/celljuke/next-bender"
  },
  "bugs": {
    "url": "https://github.com/celljuke/next-bender/issues"
  },
  "homepage": "https://github.com/celljuke/next-bender#readme",
  "private": false,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "postinstall": "prisma generate",
    "db:setup": "./scripts/setup-db.sh",
    "db:start": "docker-compose up -d postgres",
    "db:stop": "docker-compose stop postgres",
    "db:restart": "docker-compose restart postgres",
    "db:logs": "docker-compose logs -f postgres",
    "db:generate": "dotenv -e .env.local -- prisma generate",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:reset": "dotenv -e .env.local -- prisma migrate reset",
    "db:seed": "dotenv -e .env.local -- prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@tanstack/react-query": "^5.90.5",
    "@trpc/client": "^11.6.0",
    "@trpc/next": "^11.6.0",
    "@trpc/react-query": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "bcryptjs": "^3.0.2",
    "better-auth": "^1.3.28",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.546.0",
    "motion": "^12.23.24",
    "next": "15.5.6",
    "next-themes": "^0.4.6",
    "prisma": "^6.17.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "virtua": "^0.45.3",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^10.0.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
